# Environment Variables Template
# Copy this file to .env.dev or .env.prod and fill in your actual values

# Database Configuration
POSTGRES_DB=
POSTGRES_USER=
POSTGRES_PASSWORD=
POSTGRES_PORT=
POSTGRES_HOST=

# PostgreSQL Connection URL
POSTGRES_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}

# Redis Configuration
REDIS_PORT=6379
REDIS_HOST=

# Redis Connection URL
REDIS_URL=

# API Keys
API_KEY=
SECRET_KEY=

# Application Settings
DEBUG=
LOG_LEVEL=
ENVIRONMENT=

# Go API Service Settings
API_PORT=
GO_LOG_LEVEL=

# Docker Configuration
DOCKER_TAG=
API_DOCKER_IMAGE=
WORKERS_DOCKER_IMAGE=

# Build Metadata (set by Makefile)
VERSION=
COMMIT=
DATE=
